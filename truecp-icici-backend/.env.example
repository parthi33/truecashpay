# Local dev port (prod: 443 behind Nginx)
PORT=3000

# Replace with ICICI sandbox base URL from portal docs
ICICI_BASE_URL=https://apitest.icicibank.com/REPLACE_ME

# ICICI headers/keys
ICICI_APIKEY=REPLACE_ME
ICICI_PARTNER_ID=

# RSA public key from ICICI (PEM, keep \n)
ICICI_PUBLIC_KEY_PEM="-----BEGIN PUBLIC KEY-----\nREPLACE\n-----END PUBLIC KEY-----"

# If bank encrypts responses back (often plain JSON) add your private key:
OUR_PRIVATE_KEY_PEM="-----BEGIN PRIVATE KEY-----\nOPTIONAL\n-----END PRIVATE KEY-----"

# Optional callback URL
CALLBACK_URL=https://your-domain.com/icici/callback
